{
  "labels": ["dependencies"],
  "assigneesFromCodeOwners": true,
  "packageRules": [
  {
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "all non-major dependencies",
      "groupSlug": "all-minor-patch"
    },
    {
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "major"
      ],
      "groupName": "all major dependencies",
      "groupSlug": "all-major"
    }
  ],
  "extends": [
    "config:base"
  ]
}
